class Human{

    constructor() {
        this.sound = "noise";
    }

    printSound() {
        console.log(this.sound);
    }
} 

class Person extends Human {
    
    constructor() {
      super();
        this.name = 'Max';

    }
    printName() {
        console.log(this.name);
    }

}

const p3 = new Person();
p3.printName();
p3.printSound();

const p2 = new Person();
p2.printName();
p2.printSound();